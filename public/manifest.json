{
  "name": "Clipboard Manager",
  "short_name": "Clipboard Manager",
  "description": "Clipboard Manager",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
   "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["static/js/content.js"]
    }
  ],
  "permissions": ["storage"],
  "host_permissions": [
    "*://*/*"
  ]
}
